{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${mod_version}",
    "name": "${mod_name}",
    "description": "${mod_desc}",
    "authors": [ "${mod_author}" ],
    "contact": {
        "homepage": "${mod_homepage}",
        "sources": "${mod_source}",
        "issues": "${mod_issues}"
    },
    "license": "${mod_license}",
    "icon": "${mod_id}_icon.png",
    "environment": "*",
    "accessWidener": "${mod_id}.accesswidener",
    "entrypoints": {
        "main": [
            "com.mrcrayfish.furniture.refurbished.FurnitureMod"
        ],
        "client": [
            "com.mrcrayfish.furniture.refurbished.client.ClientFurnitureMod"
        ],
        "fabric-datagen": [
            "com.mrcrayfish.furniture.refurbished.FurnitureMod"
        ],
        "jei_mod_plugin": [
            "com.mrcrayfish.furniture.compat.jei.Plugin"
        ]
    },
    "mixins": [
        "${mod_id}.fabric.mixins.json",
        "${mod_id}.common.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${fabric_loader_version}",
        "fabric": "*",
        "minecraft": "${minecraft_version}",
        "java": ">=17",
        "framework": ">=${framework_version}"
    },
    "conflicts": {
        "mighty_mail": "*"
    },
    "custom": {
        "configured": {
            "configs": [],
            "background": "minecraft:textures/block/dirt.png"
        },
        "catalogue": {
            "banner": "${mod_id}_banner.png",
            "background": "${mod_id}_background.png"
        },
        "framework": {
            "containers": [
                "com.mrcrayfish.furniture.refurbished.core"
            ],
            "configs": [
                "com.mrcrayfish.furniture.refurbished.Config"
            ]
        }
    }
}
  